{
  "name": "gmi-design-system",
  "version": "0.0.11",
  "description": "design system for gmi(gong-mu-in)",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./icon": {
      "require": "./dist/icon/index.js",
      "import": "./dist/icon/index.mjs",
      "types": "./dist/icon/index.d.ts"
    },
    "./component": {
      "require": "./dist/component/index.js",
      "import": "./dist/component/index.mjs",
      "types": "./dist/component/index.d.ts"
    },
    "./global.css": "./dist/global.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "dependencies": {
    "react-aria-components": "1.3.1",
    "react-hot-toast": "2.4.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-flow": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@emotion/hash": "^0.9.2",
    "@eslint/js": "^9.8.0",
    "@storybook/addon-designs": "^8.0.3",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-storysource": "^8.2.6",
    "@storybook/builder-vite": "^8.2.6",
    "@storybook/cli": "^8.2.6",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/preset-create-react-app": "^8.2.7",
    "@storybook/react": "^8.2.6",
    "@storybook/react-vite": "^8.2.6",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vanilla-extract/css": "^1.15.3",
    "@vanilla-extract/recipes": "^0.5.3",
    "@vanilla-extract/webpack-plugin": "^2.3.11",
    "babel-loader": "^9.1.3",
    "common-path-prefix": "^3.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "storybook": "^8.2.6",
    "style-loader": "^4.0.0",
    "svgo": "^3.3.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build": "webpack --config webpack.config.cjs",
    "start": "webpack serve --config webpack.config.cjs --mode development",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "svgr-icon": "svgr --out-dir ./src/icon/default -- ./src/icon/prepare",
    "refactor-icon": "node refactorIcons.cjs",
    "auto-icon": "svgr --out-dir ./src/icon/default -- ./src/icon/prepare && npm run refactor-icon"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnd-side-project/dnd-11th-3-frontend.git"
  },
  "keywords": [
    "design-system"
  ],
  "author": "dohui-son, sujin-kim",
