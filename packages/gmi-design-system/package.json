{
  "name": "gmi-design-system",
  "version": "1.0.0",
  "description": "design system for gmi(gong-mu-in)",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./icon": {
      "require": "./dist/icon/index.js",
      "import": "./dist/icon/index.mjs",
      "types": "./dist/icon/index.d.ts"
    },
    "./component": {
      "require": "./dist/component/index.js",
      "import": "./dist/component/index.mjs",
      "types": "./dist/component/index.d.ts"
    },
    "./global.css": "./dist/global.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "dependencies": {
    "react-aria-components": "1.3.1",
    "react-hot-toast": "2.4.1",
    "storybook-addon-aria-live": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@emotion/hash": "^0.9.2",
    "@eslint/js": "^9.8.0",
    "@storybook/addon-designs": "^8.0.3",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-storysource": "^8.2.6",
    "@storybook/builder-vite": "^8.2.6",
    "@storybook/cli": "^8.2.6",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/preset-create-react-app": "^8.2.7",
    "@storybook/react": "^8.2.6",
    "@storybook/react-vite": "^8.2.6",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vanilla-extract/css": "^1.15.3",
    "@vanilla-extract/recipes": "^0.5.3",
    "@vanilla-extract/vite-plugin": "^4.0.13",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "storybook": "^8.2.6",
    "svgo": "^3.3.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen-icon": "svgr --out-dir ./src/icon/default --index-template ./tool/svgrIndexTemplate.cjs -- ./src/icon/asset"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dnd-side-project/dnd-11th-3-frontend.git"
  },
  "keywords": [
    "design-system"
  ],
  "author": "dohui-son, sujin-kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dnd-side-project/dnd-11th-3-frontend/issues"
  },
  "homepage": "https://github.com/dnd-side-project/dnd-11th-3-frontend#readme"
