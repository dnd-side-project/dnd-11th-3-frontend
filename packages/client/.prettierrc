{
   "semi": false,
   "singleQuote": true,
   "tabWidth": 3,
   "trailingComma": "all",
   "importOrderSeparation": true,
   "importOrderSortSpecifiers": true,
   "importOrderParserPlugins": ["typescript", "jsx"],
   "endOfLine": "auto",
   "importOrder": [
      "react*",
      "next*",
      "<THIRD_PARTY_MODULES>",
      "^@lib/(.*)$",
      "^[../]",
      "^[./]"
   ]
}
